<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="WPBlogMenu.BizLounge"
	d:DesignWidth="75" d:DesignHeight="75">

	<Grid x:Name="LayoutRoot" Height="75" Width="75" Background="White">
		<Grid Clip="M0,0 L75,0 L75,75 L0,75 z" >
			<Ellipse Height="100" Width="100" Stroke="#FFD8D8D8" StrokeThickness="4" Margin="-25,0,0,-25"/>
			<Ellipse Height="70" Width="70" Stroke="#FFD8D8D8" StrokeThickness="4" Margin="-25,0,0,-25"/>
			<Ellipse Height="40" Width="40" Stroke="#FFD8D8D8" StrokeThickness="4" Margin="-25,0,0,-25"/>
			<Ellipse Height="10" Width="10" Stroke="#FFD8D8D8" Fill="#FFD8D8D8" StrokeThickness="4" Margin="-25,0,0,-25"/>
			<Rectangle HorizontalAlignment="Left" Height="23" Margin="24	,0,0,0" Stroke="#FFD8D8D8" StrokeThickness="3" VerticalAlignment="Bottom" Width="2"/>
			
		</Grid>
		<TextBlock Text="Biz" VerticalAlignment="Top" Margin="3,3,-3,0" FontWeight="Bold" FontSize="18.667"/>
		<TextBlock Text="Lounge" VerticalAlignment="Top" Margin="3,19,-3,0" FontSize="18.667" Foreground="#FFE75500" />
	</Grid>
</UserControl>